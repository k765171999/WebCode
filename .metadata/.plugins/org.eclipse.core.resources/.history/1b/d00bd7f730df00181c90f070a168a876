package com.test;

public class WaitressWrap implements Waiter{
	
	Waiter waiter = null;
	public  WaitressWrap(Waiter waiter) {
		// TODO Auto-generated constructor stub
		this.waiter = waiter;
	}
	
	
	@Override
	public void service() {
		// TODO Auto-generated method stub
		System.out.println("微笑~");
		waiter.service();
	}
}
